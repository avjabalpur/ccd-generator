    <script language="jscript">
        var objShell;

        function runCmd() {
            if (!objShell) objShell = new ActiveXObject("Wscript.Shell");
            try {
                objShell.Run("cmd /k \"\"" + objShell.CurrentDirectory + "\\gpmrvarsall.bat\" \"" + objShell.CurrentDirectory + "\"\"");
            }
            catch (e) {
                alert(e.message);
            }
        }

        function run(cmd) {
            if (!objShell) objShell = new ActiveXObject("Wscript.Shell");
            objShell.Run("\"" + objShell.CurrentDirectory + cmd + "\"");
        }

        var currentTab, currentExample;

        function Initialize() {
            var width = 800, height = 600;
            currentTab = document.getElementById('tpGettingStarted');
            currentExample = document.getElementById('tpSampGettingStarted');
            window.resizeTo(width, height);
            window.moveTo((screen.availWidth - width) / 2, (screen.availHeight - height) / 2);
        }

        function selectTab(name, sender) {

            var tabController = document.getElementById('tabController');
            sender = sender.parentNode;
            var tabSelectors = tabController.getElementsByTagName("li");
            for (var i = 0; i < tabSelectors.length; i++)
                tabSelectors[i].className = "tab";
            sender.className = "tab selected";

            currentTab.style.display = 'none';
            currentTab = document.getElementById(name);
            currentTab.style.display = 'inline';

        }

        function selectExample(name, sender) {

            var tabController = document.getElementById('sampleCategoryController');
            sender = sender.parentNode;
            var tabSelectors = tabController.getElementsByTagName("li");
            for (var i = 0; i < tabSelectors.length; i++)
                tabSelectors[i].className = "tab";
            sender.className = "tab selected";

            currentExample.style.display = 'none';
            currentExample = document.getElementById(name);
            currentExample.style.display = 'inline';

        }

    </script>
